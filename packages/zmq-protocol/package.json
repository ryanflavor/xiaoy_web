{
  "name": "@xiaoy/zmq-protocol",
  "version": "0.1.0",
  "private": true,
  "description": "ZMQ protocol handlers and type definitions",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@xiaoy/shared-types": "file:../shared-types"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  }
}